extends ../layout


block content
    .container
        table.table.table-hover.table-bordered
            thead
                tr
                    th 名字
                    th 时间
            tbody
                each item in users
                    tr(id="item-id-#{item._id}")
                        td #{item.name}
                        td #{moment(item.meta.createAt).format('MM/DD/YYYY')}

